package ch22;

class Human{
	void showInfo() {
		System.out.println("All human beings are equal.");
	}
}

class Asian extends Human{
	@Override
	void showInfo() {
		System.out.println("Lee is an Asian");
	}
}

public class Main {
	public static void main(String[] args) {
		
		Human test = new Asian();
		test.showInfo();
		
		
	}
}
