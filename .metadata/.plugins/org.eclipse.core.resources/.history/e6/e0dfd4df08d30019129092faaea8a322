package ch09;

public class Array {

	public static void main(String[] args) {
		int[] numbers = new int[3];
		//int 타입의 배열을 numbers라는 변수에 담는다
		// new int[3];  3개의 배열;
		// numbers라는 변수에 3개의 원소가 있다.
		
		numbers[0]=4;
		numbers[1]=10;
		numbers[2]=20;
		System.out.println(numbers[0]);
		System.out.println(numbers[1]);
		System.out.println(numbers[2]);
		numbers[3]=20;
		// 갯수 초과
	}

}
