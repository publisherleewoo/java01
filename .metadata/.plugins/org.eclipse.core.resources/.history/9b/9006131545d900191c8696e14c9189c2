package ch22;

class Human{
	void showInfo() {
		System.out.println("All human beings are equal.");
	}
}

class Asian extends Human{
	@Override
	void showInfo() {
		System.out.println("Lee is an Asian");
	}
	void greeting() {
		System.out.println("Hello > <b");
	}
}

public class Main {
	public static void main(String[] args) {
		
		Asian test = new Asian();
		test.greeting();
	
	}
}
