package ch10;

 

public class Methods {
	public static void main(String[] args) {
		Person person1 = new Person(); //new는 클래스를 실체화할때 쓰는 예약어
		person1.name = "Lee";
		person1.booldType = "B";
	
		Person person2 = new Person();
		person2.name="Jera";
		person2.booldType="b";
		
		System.out.println(person1.name);
		System.out.println(person2.name);
	}
}
