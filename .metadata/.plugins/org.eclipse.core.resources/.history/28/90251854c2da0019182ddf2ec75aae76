package ch27;

import java.util.ArrayList;

class Nation{
	private String name;
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return this.name;
	}
	
}

class City extends Nation{
	
}


public class Main {
	public static void main(String[] args) {
		 Nation nation1 = new Nation();
		 Nation nation2 = new Nation();
		 
		 nation1.setName("korea");
		 nation2.setName("japan");
		  
		 ArrayList arrNation = new ArrayList();
		 arrNation.add(nation1);
		 arrNation.add(nation2);
		 System.out.println(arrNation);
		
		 City city1 = new City();
		 City city2 = new City();
		 
		 city1.setName("서울");
		 city2.setName("도쿄");
		
		 System.out.println(city1.getName());
		 System.out.println(city2.getName());
		 
		 
	}
}
