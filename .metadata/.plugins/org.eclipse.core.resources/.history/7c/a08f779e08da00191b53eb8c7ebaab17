package ch27;

class Person<T>{
	T Name;
	
	Person(){
		System.out.println("사람생성자");
	}
	private void setName(T value) {
		this.Name = value;
	}
	private T getName() {
		return this.Name;
	}
	
}


public class Main {
	public static void main(String[] args) {
		Person<String> p1 = new Person<String>();
		
		
		
	}
}
